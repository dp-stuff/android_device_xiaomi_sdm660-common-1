<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds.  Do not translate. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Battery Health -->
    <!-- Flag indicating whether we should enable smart battery. -->
    <bool name="config_smart_battery_available">false</bool>
    <bool name="config_supportBatteryHealth">false</bool>
    <integer name="config_smartChargingBatteryLevel">100</integer>
    <integer name="config_smartChargingBatteryResumeLevel">15</integer>
    <string name="config_batChargeCycle">/sys/class/power_supply/battery/cycle_count</string>
    <string name="config_batDesCap">/sys/class/power_supply/bms/charge_full_design</string>
    <string name="config_batCurCap">/sys/class/power_supply/bms/charge_full</string>

    <!-- Sets the package name for device parts-->
    <string name="configdevicepartspackagename" translatable="false">org.lineageos.settings.device</string>
    <string name="configdevicepartspackageactivity" translatable="false">org.lineageos.settings.device.DeviceSettingsActivity</string>
    <string name="config_customDozePackage">org.lineageos.settings/org.lineageos.settings.doze.DozeSettingsActivity</string>

    <!-- Sets the paths that are needed for SystemInfo-->
    <string name="config_cpu_temp_path" translatable="false">/sys/class/thermal/thermal_zone0/temp</string>
    <string name="config_battery_temp_path" translatable="false">sys/class/power_supply/battery/temp</string>
    <string name="config_gpu_clock_path" translatable="false">/sys/kernel/gpu/gpu_clock</string>
    <string name="config_gpu_busy_path" translatable="false">/sys/kernel/gpu/gpu_busy</string>
    <string name="config_battery_level_path" translatable="false">/sys/class/power_supply/battery/capacity</stri

</resources>

